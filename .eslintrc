{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "quotes": [
      2,
      "single"
    ],
    "indent": 2,
    "no-multi-spaces": [
      2,
      {
        "exceptions": {
          "VariableDeclarator": true
        }
      }
    ],
    "no-underscore-dangle": 2,
    "eol-last": [
      0
    ],
    "key-spacing": [
      2,
      {
        "align": "value",
        "beforeColon": false,
        "afterColon": true
      }
    ]
  }
}